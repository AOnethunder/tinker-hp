parameters    ../params/amoebabio09
integrator baoabrespa1  # Langevin respa1 baoab integrator
verbose
a-axis                        54.99
b-axis                        41.91
c-axis                        41.91
vdw-cutoff                     9.0
ewald
ewald-cutoff                    7.0
randomseed 12345
polar-alg 5   # DC-JI/DIIS for full timesteps polarization
polar-algshort 3  # TCG for short range intermediate timestep polarization
tcgordershort 1   # 1st order of TCG for short range polarization
tcgprecshort 1    # diagonal preconditioner
tcgguessshort 0   # no guess
tcgpeekshort 0    # no peek step
heavy-hydrogen    # Hydrogen Mass Repartitioning
